<%= form_for Rating.new do |f| %>
    <div class="field">
      <%= f.hidden_field :ambulance_item_id, :value => params[:id] %>

      <%= f.label :comment %><br />
      <%= f.text_field :comment %>
    </div>
    <div class="field">
      <%= f.label :value %><br />
      <%= f.text_field :value %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>